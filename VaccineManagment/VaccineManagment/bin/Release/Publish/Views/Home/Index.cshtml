<script>
    var data = [
        { y: '2014', a: 50, b: 90 },
        { y: '2015', a: 65, b: 75 },
        { y: '2016', a: 50, b: 50 },
        { y: '2017', a: 75, b: 60 },
        { y: '2018', a: 80, b: 65 },
        { y: '2019', a: 90, b: 70 },
        { y: '2020', a: @ViewBag.VacChild, b:@ViewBag.Polio  }
        
    ],
        config = {
            data: data,
            xkey: 'y',
            ykeys: ['a', 'b'],
            labels: ['Total Vaccinated Childs  ', 'Polio Vaccination '],
            fillOpacity: 0.6,
            hideHover: 'auto',
            behaveLikeLine: true,
            resize: true,
            pointFillColors: ['#ffffff'],
            pointStrokeColors: ['black'],
            lineColors: ['gray', 'red']
        };
    config.element = 'area-chart';
    Morris.Area(config);
    config.element = 'line-chart';
    Morris.Line(config);
    config.element = 'bar-chart';
    Morris.Bar(config);
    config.element = 'stacked';
    config.stacked = true;
    Morris.Bar(config);
    Morris.Donut({
        element: 'pie-chart',
        data: [
            { label: "Friends", value: 30 },
            { label: "Allies", value: 15 },
            { label: "Enemies", value: 45 },
            { label: "Neutral", value: 10 }
        ]
    });
